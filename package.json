{
  "name": "vadym-website",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 5050",
    "lint": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lintfix": "npm run lint --fix",
    "deploy": "npm run build && echo 'vadym.ml' > dist/CNAME && git add dist -f && git commit -m \"New version\" && git subtree push --prefix dist origin gh-pages"
  },
  "dependencies": {
    "@mdi/js": "^6.6.96",
    "mdi-vue": "^3.0.12",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.0.24",
    "vite": "^2.9.5"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "bracketSameLine": true,
    "bracketSpacing": true,
    "trailingComma": "none"
  }
}
